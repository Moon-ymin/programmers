class Solution {
    public String solution(String my_string, int n) {
        String answer = "";
        
        for( int i=0; i<my_string.length(); i++) {
            for (int j=0; j<n; j++) {
                answer += my_string.charAt(i);
            }
        }
        return answer;
    }
}

/*
반복문은 제대로 씀
문자열 answer에 +하는 걸로 문자 넣을 수 있음
*/
