import java.util.Arrays;

class Solution {
    public int solution(int[] array) {
        int answer = 0;
        
        Arrays.sort(array);  // 
        answer = array[ array.length / 2];
        return answer;
    }
}

/* 배열 정렬
오름차순 정렬 : Arrays.sort( ~배열~ )
내림차순 정렬 : Collections.reverseOrder( ~배열~ )
*/
